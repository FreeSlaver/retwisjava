<h2>Tweets</h2>
{#因为是在遍历中，所以用id没用#}
{% for tweet in tweetList %}
    <div class="aside tweet-one">
        <a href="#"><img src="/img/pic10.jpg"></a>
        <div class="tweet-content">
            <h5>{{ tweet.userName }}</h5>
            <span>{{ tweet.userId }}</span>
            <span>{{ tweet.id }}</span>
            <div onclick="editable()">{{ tweet.content }}</div>
            <i class="fa fa-comment pointer" aria-hidden="true"></i>
            <i class="fa fa-heart pointer" aria-hidden="true"></i>
            <!--这里是登录以后才有-->
            {#<i class="fa fa-pencil pointer" aria-hidden="true" onclick="editTweet({{ tweet.id }},{{  }})"></i>#}
            <i class="fa fa-trash pointer" aria-hidden="true" onclick="delTweet({{ tweet.id }})"></i>
        </div>
    </div>
{% endfor %}

